{
  "name": "peanut",
  "version": "0.0.1",
  "description": "Multi-purpose Discord Bot.",
  "private": true,
  "scripts": {
    "build": "npx tsc",
    "develop": "npx tsc --watch & nodemon build/app.js",
    "start": "node build/app.js",
    "lint": "npx eslint src/* --ext .ts",
    "lint:fix": "npx eslint src/* --ext .ts --fix",
    "generate": "graphql-codegen --config codegen.yml -r dotenv/config"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ANTOSzbk/peanut.git"
  },
  "keywords": [
    "discord",
    "bot",
    "typescript",
    "multi",
    "purpose",
    "gaming",
    "integration"
  ],
  "author": "ANTOSzbk <adi@antoszbk.xyz>",
  "license": "MIT",
  "dependencies": {
    "@naval-base/ms": "^3.1.0",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "common-tags": "^1.8.0",
    "discord-akairo": "^8.0.0",
    "discord.js": "^12.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.1.0",
    "graphql-tag": "^2.10.3",
    "node-emoji": "^1.10.0",
    "node-fetch": "^2.6.1",
    "prom-client": "^12.0.0",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.13.5",
    "@graphql-codegen/introspection": "^1.15.3",
    "@graphql-codegen/typescript": "1.13.5",
    "@graphql-codegen/typescript-resolvers": "^1.15.3",
    "@types/common-tags": "^1.8.0",
    "@types/diff": "^4.0.2",
    "@types/express": "^4.17.6",
    "@types/graphql": "^14.5.0",
    "@types/node": "^13.13.12",
    "@types/node-emoji": "^1.8.1",
    "@types/node-fetch": "^2.5.7",
    "@types/ws": "^7.2.5",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "hasura-cli": "^1.2.2",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "eslintConfig": {
    "extends": "prettier/node",
    "globals": {
      "BigInt": "writable"
    }
  }
}
